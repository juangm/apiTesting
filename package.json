    "got": "^12.1.0",
    "mocha": "^10.0.0",
    "prettier": "^2.0.5",
    "ts-node": "^10.8.2",
    "tslint": "^6.1.1",
    "typescript": "^4.7.4"
  },
  "comments": {
    "@types/chai": "Type definitions for chai.",
    "@types/got": "Type definitions for got.",
